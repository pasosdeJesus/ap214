<% @meses = [
  ["ENERO", 1], 
  ["FEBRERO", 2], 
  ["MARZO", 3], 
  ["ABRIL", 4], 
  ["MAYO", 5], 
  ["JUNIO", 6], 
  ["JULIO", 7], 
  ["AGOSTO", 8], 
  ["SEPTIEMBRE", 9], 
  ["OCTUBRE", 10], 
  ["NOVIEMBRE", 11], 
  ["DICIEMBRE", 12]] %>
<% @dias = *(1..31) %>
<div class="row">
  <div class="col-sm-3">
    <% @anios = *(1900..Date.today.year) %>
    <%= p.input :anionac, 
      collection: @anios %>
  </div>
  <div class="col-md-3">
    <%= p.input :mesnac, collection: @meses %>
  </div>
  <div class="col-md-3">
    <%= p.input :dianac, collection: @dias %>
  </div>
  <div class="col-md-3">
    <%= render partial: 'msip/personas/campo_sexo', locals: {f: p} %>
  </div>
</div>
